{
	"name": "tunnel-broker",
	"version": "1.0.0",
	"description": "Node.js tunnel broker server using Hono framework",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"docker:build": "docker build -t logoutrd/tunnel-broker .",
		"docker:run": "docker run -p 3000:3000 logoutrd/tunnel-broker",
		"push": "docker push logoutrd/tunnel-broker",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@hono/node-server": "^1.12.0",
		"@hono/node-ws": "^1.2.0",
		"express": "^5.1.0",
		"hono": "^4.9.0",
		"ssh2": "^1.16.0",
		"ws": "^8.18.0"
	},
	"devDependencies": {
		"@types/express": "^5.0.3",
		"@types/node": "^20.14.0",
		"@types/ssh2": "^1.15.5",
		"@types/ws": "^8.5.10",
		"tsx": "^4.7.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
